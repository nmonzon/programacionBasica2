package ar.edu.unlam.pb2.dado; /*accedo a este nivel*/
import java.util.Random;

public class Dado {
	
	private Integer caras; 

	public Integer lanzar() { 
		
		Random numrandom = new Random();
		caras = numrandom.nextInt(5) + 1;
		return caras;
	}
}

/*
